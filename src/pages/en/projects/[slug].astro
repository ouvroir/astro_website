---
export async function getStaticPaths() {
  let projects = Astro.fetchContent('/src/_projets/*-en.md');

  return projects.map((projet) => {
        return {
            params: { slug: projet.slug },
            props: {projet} };
    });
}

import ProjectLayout from '/src/layouts/ProjectLayout.astro';

const {projet} = Astro.props;

---
<ProjectLayout content={projet}>
</ProjectLayout>

